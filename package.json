{
  "name": "@fastify/boileplate",
  "version": "2.43.0",
  "type": "module",
  "private": true,
  "description": "Fastify boilerplate",
  "main": "dist/server.js",
  "author": {
    "name": "Marco Asta",
    "email": "marcoasta55@gmail.com"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup",
    "clean": "rimraf dist",
    "coverage": "cross-env NODE_ENV=test vitest  --coverage ",
    "debug": "cross-env NODE_ENV=development tsx watch ./src/server.ts --inspect",
    "dev": "cross-env NODE_ENV=development tsx watch ./src/server.ts",
    "docker:build": "docker build -f Dockerfile -t authservice-api .",
    "docker:build-nocache": "docker build --progress=plain --no-cache -f Dockerfile -t authservice-api .",
    "format": "prettier \"**/src/**/*.{js,json,ts,css,scss,vue,html,md}\" --write",
    "lic": "pnpm lic:json && pnpm lic:md",
    "lic:json": "npx license-checker-rseidelsohn --json --production --excludePrivatePackages --out ./src/licenses/licenses.json",
    "lic:md": "npx license-checker-rseidelsohn --markdown --production --excludePrivatePackages --out ./src/licenses/licenses.md",
    "lint": "eslint --cache --cache-location ./eslint-cache --no-error-on-unmatched-pattern \"src/**/*.{ts,vue}\" --fix -o eslint-report.html -f html",
    "preview": "pnpm build && pnpm serve",
    "serve": "cross-env NODE_ENV=production tsx ./dist/server.js",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:config": "cross-env NODE_ENV=test vitest configService",
    "test:ui": "cross-env NODE_ENV=test vitest --ui",
    "test:unit": "cross-env NODE_ENV=test vitest --coverage"
  },
  "dependencies": {
    "@autotelic/pino-seq-transport": "^0.1.0",
    "@clabnet/configs-ts": "^0.1.8",
    "@fastify/autoload": "^6.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/env": "^5.0.1",
    "@fastify/mongodb": "^9.0.1",
    "@fastify/one-line-logger": "^2.0.0",
    "@fastify/sensible": "^6.0.1",
    "@fastify/swagger": "9.0.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@paralect/node-mongo": "^3.2.0",
    "axios": "^1.7.7",
    "cli-color": "^2.0.4",
    "close-with-grace": "2.1.0",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "detect-port": "^1.6.1",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "fastify": "^5.0.0",
    "fastify-favicon": "^5.0.0",
    "fastify-guard": "^2.0.0",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "5.0.1",
    "fastify-zod": "^1.4.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "guid-typescript": "^1.0.9",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "node-dependency-injection": "^3.1.2",
    "pino": "^9.4.0",
    "pino-loki": "^2.3.1",
    "pino-pretty": "^11.2.2",
    "rss-parser": "^3.13.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.3",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@clabnet/configs-eslint-ts": "^0.1.10",
    "@clabnet/configs-prettier": "^0.1.15",
    "@types/cli-color": "^2.0.6",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitest/coverage-istanbul": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "eslint": "^9.11.1",
    "http-errors": "^2.0.0",
    "mime-types": "^2.1.35",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "5.6.2",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1",
    "vitest-canvas-mock": "^0.3.3"
  },
  "engines": {
    "node": "21.7.3",
    "pnpm": ">=9.0.0"
  }
}